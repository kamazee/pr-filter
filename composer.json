{
  "name": "kamazee/pr-filter",
  "description": "Tool for narrowing code quality tools reporting for pull requests",
  "license": "MIT",
  "keywords": ["filter", "diff", "checkstyle", "phpcs", "phan", "pull request"],
  "require": {
    "php": "^7.1 || ^8.1",
    "ptlis/diff-parser": "^0.6",
    "mnapoli/silly": "^1.7",
    "php-di/php-di": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3.2",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Kamazee\\PrFilter\\": "src"
    }
  },
  "bin": [
    "bin/prf",
    "bin/prf.php"
  ]
}
